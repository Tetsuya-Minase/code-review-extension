{
  "name": "code-review-ai",
  "version": "1.0.0",
  "description": "GitHubのコードレビューを支援するAIツール",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build --mode content",
    "dev": "vite build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --config eslint.config.js --no-warn-ignored",
    "lint:fix": "eslint src/**/*.ts --config eslint.config.js --fix --no-warn-ignored"
  },
  "keywords": [
    "chrome-extension",
    "github",
    "code-review",
    "ai",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.251",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-chrome-extension": "^0.0.7"
  }
}
